RewriteEngine On


RewriteRule ^sportlethen/([^/]+)$ inc/sportlethen.php?clubs_genre=$1 [QSA,L]
RewriteRule ^sportlethen/([^/]+)/([^/]+)$ inc/club/t_club.php?club=$2 [QSA,L]
RewriteRule ^sportlethen/([^/]+)/([^/]+)/event/([^/]+)$ inc/club/t_event.php?club_id=$1&event_id=$2 [QSA,L]

RewriteRule ^health-wellbeing/news/([^/]+)$ inc/news/t_news.php?id=$1 [QSA,L]

RewriteRule ^inc/(.*)$ route.php
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.*)$ route.php [QSA,L]
